;; 1. Based on: m26
;; 2. Description: TGI_natural_growth_calculation_all_rows
;; x1. Author: user
$PROBLEM TGI

$INPUT ID TREAT TIME DV DOSE CMT TID AMT EVID OCC

$DATA data_u.csv IGNORE=@

$SUBROUTINE ADVAN6 TRANS=1 TOL=9

$MODEL 
 COMP=(DOSE,DEFDOSE)
 COMP=(TUM,DEFOBS)
 

$PK
"FIRST 
" USE PRCOM_INT, ONLY: IMAX 

KG=THETA(1)*EXP(ETA(1))
A_0(2)=THETA(2)+ETA(2)
BASE=THETA(2)+ETA(2)



" IMAX=3000000 
 
$DES
DADT(1)=0
DADT(2)=KG*A(2)

$ERROR
IPRED=F
Y=F*(1+EPS(1)) + EPS(2)

$THETA
(0, 0.05) ; kg
(0, 270) ; base
 
$OMEGA
1.4 ; kg
5 ;base
 
$SIGMA
0.339 ;eps1
0.995 ;eps2

$EST METHOD=1 INTER MAXEVAL=9999 PRINT=1 NOABORT
$COV MATRIX=S
$TABLE ID TREAT TIME DV DOSE CMT TID AMT EVID OCC IPRED KG BASE CWRES NOPRINT ONEHEADER FILE=m27.tab
